extends layout

block content
  h1= title
  - const Donut = ['cost','quantity','donut_type']
  - let results;
  script.
    function buy(x){
      alert('Purchased Item: ' + x)
   } 
  div(class='container-fluid')
    div(class='row')
      each results, index in Donut
        div(class='col-sm-2' onclick='buy('+index+')') #{results}