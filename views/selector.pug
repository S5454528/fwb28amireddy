extends layout

block content
  h1= title
  - let image_names = ['donut-1.jpg','donut-2.jpg','donut-3.jpg','donut-4.jpg','donut-5.png']
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]

      img(src=`images/${iname}` alt='donuts' style='width:300px; height:300px;')
      |    
  - }